package com.javaexamples;

public class Example_009_Array_Concepts {

	public static void main(String[] args) {
    int[] number  = new  int[5];// Declaration of Array 
    
    // Assign a value to each array 
    
    number[0]= 12;
    number[1]= 13;
    number[2]= 14;
    number[3]= 15;
    number[4]= 16; 
    
    
    
    
}
}