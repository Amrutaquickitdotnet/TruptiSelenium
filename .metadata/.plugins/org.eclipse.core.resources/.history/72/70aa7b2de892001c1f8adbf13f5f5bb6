package com.javaexamples;

public class Example_007_logicalOperators {
// Widening TypeCasting
	public static void main(String[] args) {
      int x = 10;
      System.out.println(x>5 && x<4);
    //  True and false = false
      // logical OR
      
      int y = 5;
      System.out.println(y>3 || y <4);
 
    //true or false = true
   
}
}